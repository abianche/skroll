{
  "name": "@skroll/tree-sitter-skroll",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf build tree-sitter-skroll.wasm skroll.dylib skroll.dylib.dSYM",
    "build": "tsc -b",
    "gen": "tree-sitter generate",
    "build:wasm": "pnpm run gen && tree-sitter build --wasm --output ./tree-sitter-skroll.wasm",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "web-tree-sitter": "0.25.10"
  },
  "devDependencies": {
    "tree-sitter-cli": "0.25.10"
  },
  "files": [
    "grammar.js",
    "queries/*",
    "src/**",
    "dist/**",
    "tree-sitter-skroll.wasm"
  ],
  "tree-sitter": [
    {
      "scope": "source.skroll",
      "injection-regex": "^skroll$",
      "file-types": [
        "skr"
      ],
      "highlights": "queries/highlights.scm",
      "indents": "queries/indents.scm"
    }
  ]
}